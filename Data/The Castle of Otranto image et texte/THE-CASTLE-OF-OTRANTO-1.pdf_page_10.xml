            <Shape><Polygon POINTS="2178 915 291 915 294 973 294 987 2186 979 2189 965 2186 946 2178 915"/></Shape>
	    <String CONTENT="The gentle timidity of her nature made her pause for some minutes at his door. She"
                    HPOS="291"
                    VPOS="915"
                    WIDTH="1898"
                    HEIGHT="72"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_c4d31071"
                    TAGREFS="LT723"
                    BASELINE="291 1031 2142 1036" 
                    HPOS="288"
                    VPOS="979"
                    WIDTH="1854"
                    HEIGHT="68">
            <Shape><Polygon POINTS="291 1031 288 1042 2137 1047 2142 1036 2140 990 291 979 291 1031"/></Shape>
	    <String CONTENT="heard him traverse his chamber backwards, and forwards with disordered steps; a"
                    HPOS="288"
                    VPOS="979"
                    WIDTH="1854"
                    HEIGHT="68"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_dd788846"
                    TAGREFS="LT723"
                    BASELINE="291 1102 2093 1102" 
                    HPOS="291"
                    VPOS="1053"
                    WIDTH="1802"
                    HEIGHT="63">
            <Shape><Polygon POINTS="291 1102 291 1116 2093 1116 2093 1102 2093 1053 291 1053 291 1102"/></Shape>
	    <String CONTENT=" mood which increased her apprehensions. She was, however, just going to beg"
                    HPOS="291"
                    VPOS="1053"
                    WIDTH="1802"
                    HEIGHT="63"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_c6f904a5"
                    TAGREFS="LT723"
                    BASELINE="291 1166 2140 1166" 
                    HPOS="291"
                    VPOS="1119"
                    WIDTH="1849"
                    HEIGHT="60">
            <Shape><Polygon POINTS="291 1166 291 1179 2137 1179 2140 1166 2137 1119 291 1119 291 1166"/></Shape>
	    <String CONTENT=" admittance, when Manfred suddenly opened the door; and as it was now twilight,"
                    HPOS="291"
                    VPOS="1119"
                    WIDTH="1849"
                    HEIGHT="60"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_b45a6d9a"
                    TAGREFS="LT723"
                    BASELINE="291 1232 2219 1226" 
                    HPOS="288"
                    VPOS="1177"
                    WIDTH="1931"
                    HEIGHT="68">
            <Shape><Polygon POINTS="291 1232 291 1245 2219 1240 2219 1226 2211 1177 288 1182 291 1232"/></Shape>
	    <String CONTENT="concurring with the disorder of his mind, he did not distinguish the person, but asked"
                    HPOS="288"
                    VPOS="1177"
                    WIDTH="1931"
                    HEIGHT="68"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_9f3a32b8"
                    TAGREFS="LT723"
                    BASELINE="291 1295 1424 1295" 
                    HPOS="291"
                    VPOS="1248"
                    WIDTH="1133"
                    HEIGHT="61">
            <Shape><Polygon POINTS="1419 1248 291 1248 291 1295 291 1309 1424 1309 1424 1295 1424 1292 1419 1248"/></Shape>
	    <String CONTENT="angrily, who it was? Matilda replied, trembling–"
                    HPOS="291"
                    VPOS="1248"
                    WIDTH="1133"
                    HEIGHT="61"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_0f2ea19c"
                    TAGREFS="LT723"
                    BASELINE="302 1394 1252 1394" 
                    HPOS="299"
                    VPOS="1339"
                    WIDTH="952"
                    HEIGHT="69">
            <Shape><Polygon POINTS="299 1391 299 1408 1251 1408 1251 1391 1251 1342 299 1339 299 1391"/></Shape>
	    <String CONTENT="&quot;My dearest father, it is l, your daughter.&quot;"
                    HPOS="299"
                    VPOS="1339"
                    WIDTH="952"
                    HEIGHT="69"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_dc7343b0"
                    TAGREFS="LT723"
                    BASELINE="291 1498 2104 1498" 
                    HPOS="291"
                    VPOS="1443"
                    WIDTH="1813"
                    HEIGHT="72">
            <Shape><Polygon POINTS="2079 1463 2052 1463 2024 1460 1997 1460 1969 1460 1942 1457 1914 1457 1887 1454 1859 1454 1832 1454 1804 1452 1777 1452 1749 1452 1733 1449 291 1443 291 1498 291 1515 2104 1515 2104 1498 2093 1476 2079 1463"/></Shape>
	    <String CONTENT="Manfred, stepping back hastily, cried, &quot;Begone! I do not want a daughter;&quot; and"
                    HPOS="291"
                    VPOS="1443"
                    WIDTH="1813"
                    HEIGHT="72"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_8d5a4a91"
                    TAGREFS="LT723"
                    BASELINE="291 1564 1865 1559" 
                    HPOS="291"
                    VPOS="1512"

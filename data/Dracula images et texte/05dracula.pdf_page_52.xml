	    <String CONTENT="one except, of course, Jonathan. You will tell him, because I would, if I were in"
                    HPOS="365"
                    VPOS="1311"
                    WIDTH="1816"
                    HEIGHT="77"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_937f1779"
                    TAGREFS="LT752"
                    BASELINE="371 1441 2186 1441" 
                    HPOS="371"
                    VPOS="1391"

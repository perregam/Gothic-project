                    HPOS="363"
                    VPOS="495"
                    WIDTH="1116"
                    HEIGHT="57"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_c4993778"
                    TAGREFS="LT752"
                    BASELINE="470 660 2184 660" 
                    HPOS="470"
                    VPOS="607"

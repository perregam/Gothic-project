                    BASELINE="368 1498 2181 1504" 
                    HPOS="365"
                    VPOS="1443"
                    WIDTH="1816"
                    HEIGHT="72">
            <Shape><Polygon POINTS="368 1498 365 1512 2175 1515 2181 1504 2162 1454 368 1443 368 1498"/></Shape>
	    <String CONTENT="Don&#39;t vou think so, dear? And I must be fair. Men like women, certainly their"
                    HPOS="365"
                    VPOS="1443"
                    WIDTH="1816"
                    HEIGHT="72"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_ebf7fa16"
                    TAGREFS="LT752"
                    BASELINE="371 1570 2178 1562" 
                    HPOS="368"
                    VPOS="1518"
                    WIDTH="1810"
                    HEIGHT="66">
            <Shape><Polygon POINTS="371 1570 371 1584 2175 1575 2178 1562 2170 1518 368 1520 371 1570"/></Shape>
	    <String CONTENT="wives, to be quite as fair as they are. And women, I am afraid, are not always"
                    HPOS="368"
                    VPOS="1518"
                    WIDTH="1810"
                    HEIGHT="66"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_18a43fe1"
                    TAGREFS="LT752"
                    BASELINE="368 1633 1064 1633" 
                    HPOS="368"
                    VPOS="1586"
                    WIDTH="696"
                    HEIGHT="61">
            <Shape><Polygon POINTS="368 1633 368 1647 1064 1647 1064 1633 1064 1586 368 1586 368 1633"/></Shape>
	    <String CONTENT=" quite as fair as they should be."
                    HPOS="368"
                    VPOS="1586"
                    WIDTH="696"
                    HEIGHT="61"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_b1cd73cd"
